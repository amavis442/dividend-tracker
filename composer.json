{
    "type": "project",
    "license": "MIT",
    "version": "1.0.1",
    "homepage": "https://gitlab.com/amavis442/dividend",
    "authors": [
        {
            "name": "Patrick Teunissen",
            "email": "patteun8@gmail.com",
            "homepage": "https://gitlab.com/amavis442",
            "role": "Developer"
        }
    ],
    "keywords": [
        "trading",
        "tracker",
        "dividends",
        "stocks"
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "^3.3",
        "babdev/pagerfanta-bundle": "^4.4",
        "beberlei/doctrineextensions": "^1.3",
        "doctrine/doctrine-bundle": "^2.13",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^2.17",
        "mtdowling/jmespath.php": "^2.8",
        "n5s/brotli": "^2.0",
        "nelmio/cors-bundle": "^2.3",
        "nesbot/carbon": "^3.8",
        "nikic/php-parser": "^5.2",
        "openspout/openspout": "^4.24",
        "pagerfanta/doctrine-orm-adapter": "^4.7",
        "pagerfanta/twig": "^4.7",
        "sabre/xml": "^4.0",
        "symfony/asset": "6.4.*",
        "symfony/asset-mapper": "6.4.*",
        "symfony/cache": "6.4.*",
        "symfony/console": "6.4.*",
        "symfony/dotenv": "6.4.*",
        "symfony/expression-language": "6.4.*",
        "symfony/flex": "^2",
        "symfony/form": "6.4.*",
        "symfony/framework-bundle": "6.4.*",
        "symfony/http-client": "6.4.*",
        "symfony/intl": "6.4.*",
        "symfony/mailer": "6.4.*",
        "symfony/mime": "6.4.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/notifier": "6.4.*",
        "symfony/options-resolver": "6.4.*",
        "symfony/process": "6.4.*",
        "symfony/property-access": "6.4.*",
        "symfony/property-info": "6.4.*",
        "symfony/proxy-manager-bridge": "6.4.*",
        "symfony/runtime": "6.4.*",
        "symfony/security-bundle": "6.4.*",
        "symfony/serializer": "6.4.*",
        "symfony/stopwatch": "6.4.*",
        "symfony/string": "6.4.*",
        "symfony/translation": "6.4.*",
        "symfony/twig-bundle": "6.4.*",
        "symfony/uid": "6.4.*",
        "symfony/ux-autocomplete": "^2.19",
        "symfony/ux-chartjs": "^2.19",
        "symfony/ux-twig-component": "^2.19",
        "symfony/validator": "6.4.*",
        "symfony/web-link": "6.4.*",
        "symfony/yaml": "6.4.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/intl-extra": "^3.13",
        "twig/twig": "^2.12|^3.0",
        "zbateson/mail-mime-parser": "^2.2"
    },
    "require-dev": {
        "behat/behat": "^3.14",
        "dama/doctrine-test-bundle": "^8.2",
        "deployer/deployer": "^7.0",
        "doctrine/coding-standard": "^9.0",
        "doctrine/doctrine-fixtures-bundle": "^3.6",
        "fakerphp/faker": "^1.23",
        "friends-of-behat/symfony-extension": "^2.6",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpspec/phpspec": "^7.5",
        "phpstan/phpdoc-parser": "^1.18",
        "phpstan/phpstan": "^1.6",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^1.2",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/browser-kit": "6.4.*",
        "symfony/css-selector": "6.4.*",
        "symfony/debug-bundle": "6.4.*",
        "symfony/maker-bundle": "^1.48",
        "symfony/phpunit-bridge": "6.4.*",
        "symfony/var-dumper": "6.4.*",
        "symfony/web-profiler-bundle": "6.4.*",
        "thecodingmachine/phpstan-strict-rules": "^1.0",
        "zenstruck/browser": "^1.8",
        "zenstruck/foundry": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "symfony/runtime": true,
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "csfix": "phpcbf --ignore=migrations/**,src/Kernel.php,bin/**",
        "cscheck": "phpcs --ignore=migrations/**,src/Kernel.php,bin/**",
        "phpstan": "phpstan analyse src/ -c phpstan.neon --level=5 --no-progress -vvv --memory-limit=1024M",
        "phpunit": "phpunit --testdox"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    }
}
